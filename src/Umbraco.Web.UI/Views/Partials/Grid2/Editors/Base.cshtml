@inherits UmbracoViewPage<GridValue.GridControl>
@{
    try
    {
        var view = Model.Editor.Render != null ? Model.Editor.Render : Model.Editor.View;

        view = view.Replace(".html", ".cshtml", StringComparison.InvariantCultureIgnoreCase);

        if (view.Contains("/") == false)
        {
            view = $"Grid2/Editors/{view}";
        }

        @Html.Partial(view, Model)
    }
    catch (Exception ex)
    {
        <pre>@ex.ToString()</pre>
    }
}
