<?xml version="1.0" encoding="utf-8"?>
<tinymceConfig>
    <commands>
        <command alias="code" name="Code" />
        <command alias="codemirror" name="Code mirror" />
        <command alias="removeformat" name="Remove format" />
        <command alias="undo" name="Undo" />
        <command alias="redo" name="Redo" />
        <command alias="cut" name="Cut" />
        <command alias="copy" name="Copy" />
        <command alias="paste" name="Paste" />
        <command alias="styleselect" name="Style select" />
        <command alias="bold" name="Bold" />
        <command alias="italic" name="Italic" />
        <command alias="underline" name="Underline" />
        <command alias="strikethrough" name="Strikethrough" />
        <command alias="alignleft" name="Justify left" />
        <command alias="aligncenter" name="Justify center" />
        <command alias="alignright" name="Justify right" />
        <command alias="alignjustify" name="Justify full" />
        <command alias="bullist" name="Bullet list" />
        <command alias="numlist" name="Numbered list" />
        <command alias="outdent" name="Decrease indent" />
        <command alias="indent" name="Increase indent" />
        <command alias="link" name="Insert/edit link" />
